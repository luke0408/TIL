{
  "name": "my-website",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "======DEVELOPMENT======": "",
    "start": "docusaurus start",
    "======BUILD======": "",
    "build": "docusaurus build",
    "======LINT======": "",
    "format": "prettier --write .",
    "format:diff": "prettier --list-different .",
    "lint": "npm run lint:style && npm run lint:spelling",
    "lint:code": "eslint --cache --report-unused-disable-directives \"**/*.{js,jsx,ts,tsx,mjs}\"",
    "lint:code:fix": "npm run lint:code -- --fix",
    "lint:spelling": "cspell \"**\" --no-progress --show-context --show-suggestions",
    "lint:spelling:fix": "npm run lint:spelling -- --words-only --unique --no-exit-code --no-summary ",
    "lint:style": "stylelint \"**/*.css\"",
    "lint:style:fix": "npm run lint:style -- --fix",
    "======DEPLOY======": "",
    "deploy": "docusaurus deploy",
    "======UTILS======": "",
    "docusaurus": "docusaurus",
    "swizzle": "docusaurus swizzle",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "prepare": "husky"
  },
  "dependencies": {
    "@docusaurus/core": "^3.9.0",
    "@docusaurus/preset-classic": "^3.9.0",
    "@docusaurus/theme-mermaid": "^3.9.0",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.0.0",
    "color": "^5.0.2",
    "prism-react-renderer": "^2.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@docusaurus/eslint-plugin": "^3.9.1",
    "@docusaurus/module-type-aliases": "3.8.1",
    "@docusaurus/tsconfig": "3.8.1",
    "@docusaurus/types": "3.8.1",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "cspell": "^9.2.1",
    "eslint": "^8.45.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-compiler": "^19.0.0-beta-40c6c23-20250301",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-regexp": "^1.15.0",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^29.0.0",
    "typescript": "~5.8.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}
